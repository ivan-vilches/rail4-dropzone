
<div class="container">
    <h1>Upload Images</h1>
    <div class="z-depth-5">
    <%= form_for(@upload, html: { multipart: true, class: "dropzone"}) do |f|  %>
    <div class="fallback">
        <%= f.file_field :image %><br>
        <%= f.submit "Upload" %>
    </div>
    <% end %>
    </div>
</div>
